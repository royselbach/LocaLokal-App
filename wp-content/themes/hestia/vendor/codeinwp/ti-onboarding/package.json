  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e assets/vue/app.js -o assets/js/bundle.js",
    "dev": "npm-run-all --parallel watchify",
    "build": "cross-env NODE_ENV=production browserify -g envify assets/vue/app.js | uglifyjs -c warnings=false -m > assets/js/bundle.js"
  },
  "devDependencies": {
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.0",
    "grunt-plugin-fleet": "codeinwp/grunt-plugin-fleet",
    "load-project-config": "~0.2.0",
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.1",
    "envify": "^3.4.1",
    "http-server": "^0.9.0",
    "node-sass": "^4.10.0",
    "npm-run-all": "^2.1.2",
    "uglify-js": "^2.5.0",
    "vueify": "^9.1.0",
    "watchify": "^3.4.0"
  },
  "dependencies": {
    "v-show-slide": "^1.6.0",
    "vue": "^2.4.2",
    "vue-clickaway": "^2.1.0",
    "vue-js-toggle-button": "^1.2.3",
    "vue-moment": "^3.2.0",
    "vue-resource": "^1.3.4",
    "vuex": "^2.4.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  }
